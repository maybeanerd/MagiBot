{
  "name": "magibot",
  "version": "3.2.71",
  "description": "MagiBot - a Discord bot to build communities",
  "main": "dist/index.js",
  "engines": {
    "node": "22.22.0"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "@discordjs/builders": "1.13.1",
    "@discordjs/rest": "2.6.0",
    "@discordjs/voice": "0.19.0",
    "@snazzah/davey": "0.1.9",
    "axios": "1.13.4",
    "blapi": "3.1.2",
    "bufferutil": "4.1.0",
    "discord-api-types": "0.38.38",
    "discord.js": "14.25.1",
    "dotenv": "17.2.3",
    "fast-folder-size": "2.4.0",
    "ffmpeg-static": "5.3.0",
    "ffprobe": "1.1.2",
    "forever": "4.0.3",
    "libsodium-wrappers": "0.8.2",
    "mongoose": "9.1.5",
    "opusscript": "0.1.1",
    "posthog-node": "5.24.7",
    "tweetnacl": "1.0.3",
    "utf-8-validate": "6.0.6"
  },
  "optionalDependencies": {
    "@discordjs/opus": "0.10.0",
    "sodium": "3.0.2",
    "zlib-sync": "0.1.10"
  },
  "devDependencies": {
    "@types/centra": "2.2.3",
    "@types/ffprobe": "1.1.8",
    "@types/node": "22.19.7",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "rimraf": "6.1.2",
    "typescript": "5.9.3"
  },
  "scripts": {
    "install-dev": "npm i --no-optional",
    "dev": "npm run build && node dist/index.js",
    "lint": "eslint \"src/**/*\"",
    "lint:fix": "eslint \"src/**/*\" --fix",
    "build": "rimraf ./dist && tsc",
    "start": "node dist/index.js",
    "start:forever": "forever start dist/index.js",
    "stop": "forever stop dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maybeanerd/MagiBot.git"
  },
  "author": "Sebastian Di Luzio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maybeanerd/MagiBot/issues"
  },
  "homepage": "https://github.com/maybeanerd/MagiBot#readme",
  "heroku-run-build-script": true
}
